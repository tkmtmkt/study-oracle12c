#
# dbca -silent -responseFile ~/share/dbca_create_TESTDB.rsp
#
[GENERAL]
RESPONSEFILE_VERSION = "12.1.0"
OPERATION_TYPE = "createDatabase"

[CREATEDATABASE]
GDBNAME = "TESTDB"
SID = "TESTDB"
TEMPLATENAME = "General_Purpose.dbc"
SYSPASSWORD = "oracle"
SYSTEMPASSWORD = "oracle"

EMCONFIGURATION = "DBEXPRESS"
EMEXPRESSPORT = "5500"

DATAFILEDESTINATION = "/data/oradata"
RECOVERYAREADESTINATION= "/u01/app/oracle/flash_recovery_area"

CHARACTERSET = "AL32UTF8"
NATIONALCHARACTERSET = "AL16UTF16"

INITPARAMS = "db_create_file_dest=/data/oradata,db_recovery_file_dest=/data/fast_recovery_area,db_recovery_file_dest_size=2048"

TOTALMEMORY = "1024"
